{
    "version": 2,
    "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
    "routes": [
      {
        "src": "/login",
        "dest": "./index.js"
      },
      {
        "src": "/register",
        "dest": "./index.js"
      },
      {
        "src": "/(.*)",
        "dest": "./index.js"
      }
    ],
    "env": {
      "DB_USER": "root",
      "DB_HOST": "localhost",
      "DB_PASSWORD": "",
      "DB_DATABASE": "plantdb"
    }
  }