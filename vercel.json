{
    "version": 2,
    "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
    "routes": [
      {
        "src": "/login",
        "dest": "./index.js"
      },
      {
        "src": "/register",
        "dest": "./index.js"
      },
      {
        "src": "/(.*)",
        "dest": "./index.js"
      }
    ],
    "headers": {
      "Access-Control-Allow-Origin": "http://localhost:5174",
      "Access-Control-Allow-Methods": "POST, GET, OPTIONS, DELETE",
      "Access-Control-Allow-Headers": "Content-Type"
    },
    "env": {
      "DB_USER": "root",
      "DB_HOST": "localhost",
      "DB_PASSWORD": "",
      "DB_DATABASE": "plantdb"
    }
  }
  